{
  "name": "infinity",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"vue-cli-service serve --mode development\" \"cd ../../backend && firebase serve\"",
    "frontserve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "firebase": "^5.8.4",
    "firebaseui": "^3.5.2",
    "v-drag-drop": "^0.2.1",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vuedraggable": "^2.19.1",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}